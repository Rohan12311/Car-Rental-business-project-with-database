<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/profile.css">
  </head>
  <body>
    <div class="containerpres">
        <div class="cardpres">
          <div class="profile-picture">
            <img src="https://images.unsplash.com/photo-1584994696678-3d739b5ac1bf?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTF8fHNwaWRlcm1hbnxlbnwwfHwwfHx8MA%3D%3D" alt="Profile Picture">
          </div>
          <h2 class="Fullname"><%= user.fullname%></h2>
          <h3 class="username">@<%=user.username%></h3>
          <p class="tagline">Lorem ipsum dolor sit amet consectetur adipisicing.</p>
          <p class="description">Lorem ipsum dolor sit, amet consectetur adipisicing elit. At!</p>
          <a href="/logout" class="button">Logout</a>
          <a href="/upload" class="button">upload</a>
          <a href="/feed" class="button">Main Page</a>
          
        </div>
    </div>
    <div class="part">
      <% user.Car.forEach(function(car){ %>
        <div class="car">
          <img src="/images/uploads/<%= car.image %>" id="carImage"></img>
          <h4 class="name"><%= car.imageText %></h4>
          <div class="seats">
              <img class="Seicon" src="/images/home page/car-chair.png"></img>
              <h4 class="Sename">5 Seats</h4>
          </div>
          <div class="speed">
              <img class="Micon" src="/images/home page/speed.png"></img>
              <h4 class="Mname">5.2 Kmpl</h4>
          </div>
          <div class="type">
              <img class="Tyicon" src="/images/home page/fuel-pump.png" alt="">
              <h4 class="Tyname">Diesel</h4>
          </div>
          <div class="Manul" id="man1">
              <img class="Maicon" src="/images/home page/steering-wheel.png" alt="">
              <h4 class="Maname">Manual</h4>
          </div>
          <div class="line"></div>
          <h4 class="price">From â‚¹300/hr</h4>
          <a href="./form"><button class="rent">Rent Now</button></a>
        </div>
      <%}) %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>